{
  "name": "validation-utils",
  "version": "4.0.2",
  "description": "Validation utilities",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest --config=jest.config.json",
    "test:ci": "npm run lint && npm run test:coverage",
    "test:ci:legacy": "npm run test:coverage",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "lint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/validation-utils.git"
  },
  "keywords": [
    "common",
    "utilities",
    "validation",
    "assertion"
  ],
  "homepage": "http://github.com/kibertoad/validation-utils",
  "dependencies": {
    "zoology": "^1.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.6",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^25.5.4",
    "prettier": "^2.0.5",
    "typescript": "3.9.3",
    "ts-jest": "^25.5.1"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ]
}
