{
  "name": "validation-utils",
  "version": "2.0.0",
  "description": "Validation utilities",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "src/index.js",
  "scripts": {
    "test": "mocha ./test",
    "test-coverage": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/validation-utils.git"
  },
  "keywords": [
    "common",
    "utilities",
    "validation",
    "assertion"
  ],
  "homepage": "http://github.com/kibertoad/validation-utils",
  "dependencies": {
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "3.19.0",
    "eslint-plugin-node": "^4.2.1",
    "eslint-html-reporter": "^0.5.2",
    "mocha": "^3.4.1",
    "nyc": "10.3.2"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "src/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./build/reports/coverage",
    "check-coverage": true,
    "lines": 95,
    "statements": 95,
    "functions": 100,
    "branches": 100
  }

}
