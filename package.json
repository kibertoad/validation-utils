{
  "name": "validation-utils",
  "version": "8.0.0",
  "description": "Validation utilities",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest",
    "test:everything": "npm run lint && npm run test:coverage",
    "test:coverage": "vitest --coverage",
    "lint": "eslint \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\" index.ts",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/validation-utils.git"
  },
  "keywords": [
    "common",
    "utilities",
    "utils",
    "validation",
    "assertion",
    "validator",
    "validate",
    "assert",
    "null",
    "type",
    "nil",
    "empty"
  ],
  "homepage": "http://github.com/kibertoad/validation-utils",
  "dependencies": {
    "zoology": "^1.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@types/node": "^18.16.19",
    "@vitest/coverage-v8": "^0.32.2",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6",
    "vitest": "^0.32.2"
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist/*"
  ]
}
