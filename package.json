{
  "name": "validation-utils",
  "version": "4.0.0",
  "description": "Validation utilities",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "del-cli dist/ && tsc",
    "test": "jest --config=jest.config.json",
    "test:ci": "npm run lint && npm run test:coverage",
    "test:coverage": "jest --config=jest.config.json --coverage",
    "lint": "eslint --format codeframe \"lib/**/*.ts\" \"test/**/*.ts\"",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\"",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/validation-utils.git"
  },
  "keywords": [
    "common",
    "utilities",
    "validation",
    "assertion"
  ],
  "homepage": "http://github.com/kibertoad/validation-utils",
  "dependencies": {
    "zoology": "^1.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "^10.14.16",
    "del-cli": "1.1.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "typescript": "^3.5.3",
    "ts-jest": "^24.0.2"
  }
}
