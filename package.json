{
  "name": "validation-utils",
  "version": "2.6.0",
  "description": "Validation utilities",
  "license": "MIT",
  "maintainers": [
    {
      "name": "Igor Savin",
      "email": "kibertoad@gmail.com"
    }
  ],
  "scripts": {
    "test": "mocha ./test",
    "test-coverage": "nyc npm test",
    "lint": "eslint .",
    "prettier": "prettier --write \"{lib,test}/**/*.{js,ts}\"",
    "test-travis": "npm run lint && nyc mocha --opts \"./mocha.opts\" test"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/kibertoad/validation-utils.git"
  },
  "keywords": [
    "common",
    "utilities",
    "validation",
    "assertion"
  ],
  "homepage": "http://github.com/kibertoad/validation-utils",
  "dependencies": {
    "lodash": "^4.17.10"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "prettier": "^1.13.7"
  },
  "nyc": {
    "description": "test coverage",
    "include": [
      "lib/**"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./build/reports/coverage",
    "check-coverage": true,
    "lines": 100,
    "statements": 100,
    "functions": 100,
    "branches": 100
  }
}
